<!--pages/check/check.wxml-->
<view>
<form bindsubmit='confirm'>

 <view class='row'>

        <view class='info'>

               <input name="building" class= 'info-input1'  placeholder="请输入宿舍楼号" />

         </view>

 </view>

 <view class='row'>

        <view class='info'>

               <input name="room" class= 'info-input' placeholder="请输入宿舍号" />

         </view>

 

 </view>

  <view class='row'>

        <view class='info'>

               <input name="scores" class= 'info-input'  placeholder="请输入评分" />

         </view>

 

 </view>
<view class='photo'>
<image src='{{photoUrl}}' mode='aspectFill'></image>
</view>
<button class='submit' bindtap="choose">拍摄图片</button>
<button class='submit' form-type='submit'>提交</button>

 </form>
 <i-toast id="toast" />
 </view>
