<!--pages/notice/notice.wxml-->
<view>
<view>
<van-tabbar active="1" bind:change="onChange">
  <van-tabbar-item icon="home-o">首页</van-tabbar-item>
  <van-tabbar-item icon="friends-o">通知</van-tabbar-item>
  <van-tabbar-item icon="setting-o">个人中心</van-tabbar-item>
</van-tabbar>
</view>

</view>
<view class="table" >
  
    <view class="record" >
    <view class="on">
       <text class="action">检查任务</text>
       <text class="get">!></text>
    </view>
    <view class="hr"></view>
    <view class="shortintro">
    <text>请{{notice.person}} 同学于{{notice.check_time}}到{{notice.building}}宿舍楼检查{{notice.id.room}} 宿舍
    检查细则为：{{notice.rule}}</text>
    </view>
  <view class="under">
       <text class="time">{{notice.check_time}}</text>
  </view>
</view>
</view>
