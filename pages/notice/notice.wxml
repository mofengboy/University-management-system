<!--pages/notice/notice.wxml-->
<view>
<view>
<van-tabbar active="1" bind:change="onChange">
  <van-tabbar-item icon="home-o">首页</van-tabbar-item>
  <van-tabbar-item icon="friends-o" info="5">通知</van-tabbar-item>
  <van-tabbar-item icon="setting-o" info="20">个人中心</van-tabbar-item>
</van-tabbar>
</view>

<view class="mask" catchtouchmove="preventTouchMove" wx:if="{{showModal}}"></view>

 

<view class="modalDlg" wx:if="{{showModal}}">


</view>
<view class="table" >
  <block wx:for="{{listData}}" wx:key="{{code}}">
    <view class="record" >
    <view class="on">
       <text class="action">检查任务</text>
       <text class="get">!></text>
    </view>
    <view class="hr"></view>
    <view class="shortintro">
    <text>请 同学于{{item.date}}  {{item.time}}到 宿舍楼检查 宿舍
    检查细则为：</text>
    </view>
  <view class="under">
       <text class="time">{{item.date}}  {{item.time}}</text>
  </view>
</view>

  </block>
</view>
</view>